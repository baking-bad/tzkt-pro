import asyncio
from postgrest_py import PostgrestClient

async def main():
  async with PostgrestClient("{{client_url}}") as client:
    r = await client \
      .auth("{{auth}}") \
      .from_("{{table_name}}") \
      .select("*") \
      .limit(5) \
      .execute()
    {{table_name}} = r.json()

asyncio.run(main())
